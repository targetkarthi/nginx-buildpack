#!/usr/bin/env bash
# bin/detect <build-dir>
# This script takes BUILD_DIR as a single argument and must return
# an exit code of 0 if the app present at BUILD_DIR can be serviced
# by this buildpack. If the exit code is 0, the script must print a
# human-readable framework name to stdout.
echo 'nginx-buildpack received $1'
exit 0
